//https://www.geeksforgeeks.org/problems/linked-list-length-even-or-odd/1

class Solution {
    public boolean isLengthEven(Node head) {
        // code here
        int count =0;
        Node curr=head;
        while(curr !=null){
            count++;
            curr=curr.next;
        }
        return count%2==0;
    }
}
